			type="gcom.gui.faturamento.GerarTxtContasProjetosEspeciaisForm" />
	</form-beans>

	<!--******************* PAGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>
		
		<action name="GerarTxtContasProjetosEspeciaisForm"
			type="gcom.gui.faturamento.ExibirGerarTxtContasProjetosEspeciaisAction"
			validate="true" 
			path="/exibirGerarTxtContasProjetosEspeciaisAction">
			
			<forward name="exibirGerarTxtContasProjetosEspeciaisAction" 
				path="/jsp/faturamento/gerar_txt_contas_projetos_especiais.jsp" />
				
		</action>
		
		<action name="GerarTxtContasProjetosEspeciaisForm"
			type="gcom.gui.faturamento.GerarTxtContasProjetosEspeciaisAction"
			validate="true" 
			path="/gerarTxtContasProjetosEspeciaisAction" />		
		
	</action-mappings>

</struts-config>