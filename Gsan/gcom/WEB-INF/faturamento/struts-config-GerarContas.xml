<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>


	<!--******************* PAGINAS GLOBAIS *******************-->
	<global-forwards />

	<action-mappings>
		
		<action
			type="gcom.gui.faturamento.ExibirGerarContasAction"
			scope="request"
			validate="true" 
			path="/exibirGerarContasAction">

			<forward name="exibirGerarContas" 
				path="/jsp/faturamento/gerar_contas.jsp" />

		</action>
		
		<action
			type="gcom.gui.relatorio.faturamento.GerarContasAction"
			validate="true" 
			path="/gerarContasAction" />
		
		
	</action-mappings>

</struts-config>