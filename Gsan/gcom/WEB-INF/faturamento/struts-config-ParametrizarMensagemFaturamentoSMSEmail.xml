<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<form-beans>
		<form-bean 
		    name="ParametrizarMensagemFaturamentoSMSEmailActionForm"
			type="gcom.gui.faturamento.ParametrizarMensagemFaturamentoSMSEmailActionForm" /> 
	</form-beans>


	<!--******************* PÃGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>
				
			
		<action name="ParametrizarMensagemFaturamentoSMSEmailActionForm"  
			type="gcom.gui.faturamento.ExibirParametrizarMensagemFaturamentoSMSEmailAction" 
			path="/exibirParametrizarMensagemFaturamentoSMSEmailAction" scope="session">
			<forward name="exibirParametrizarMensagemFaturamentoSMSEmail"
			path="/jsp/faturamento/parametrizar_mensagem_faturamento_SMS_Email.jsp" />
		</action>
		
		<action name="ParametrizarMensagemFaturamentoSMSEmailActionForm"  
			type="gcom.gui.faturamento.ParametrizarMensagemFaturamentoSMSEmailAction" 
			path="/parametrizarMensagemFaturamentoSMSEmailAction">
		</action>


	</action-mappings>
		
</struts-config>