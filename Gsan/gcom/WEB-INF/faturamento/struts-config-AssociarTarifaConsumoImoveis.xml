<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>
	<form-beans>
		<form-bean name="AssociarTarifaConsumoImoveisActionForm"
			type="gcom.gui.faturamento.consumotarifa.AssociarTarifaConsumoImoveisActionForm" />
	</form-beans>
	<!--******************* PÁGINAS GLOBAIS *******************-->
	<global-forwards />

	<action-mappings>

		<action name="AssociarTarifaConsumoImoveisActionForm"
			type="gcom.gui.faturamento.consumotarifa.ExibirAssociarTarifaConsumoImoveisAction"
			validate="true" scope="session"
			path="/exibirAssociarTarifaConsumoImoveisAction">

			<forward name="exibirAssociarTarifaConsumoImoveis"
				path="/jsp/faturamento/consumotarifa/consumo_tarifa_imoveis_associar.jsp" />

		</action>

		
		<action name="AssociarTarifaConsumoImoveisActionForm"
			type="gcom.gui.faturamento.consumotarifa.AssociarTarifaConsumoImoveisAction"
			validate="false" scope="session"
			path="/associarTarifaConsumoImoveisAction">
		</action>
		
			
			
			
			
			
	
	</action-mappings>

	<message-resources null="false"
		parameter="gcom.properties.application" />
</struts-config>