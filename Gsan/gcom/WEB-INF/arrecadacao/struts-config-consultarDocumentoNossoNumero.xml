<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<form-beans>
		<form-bean name="ConsultarDocumentoArrecadacaoNossoNumeroActionForm"
			type="gcom.gui.arrecadacao.ConsultarDocumentoArrecadacaoNossoNumeroActionForm" />
	</form-beans>

	<!--******************* PÁGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>
		<action name="ConsultarDocumentoArrecadacaoNossoNumeroActionForm"
			type="gcom.gui.arrecadacao.ExibirConsultarDocumentoArrecadacaoNossoNumeroAction"
			validate="false" path="/exibirConsultarDocumentoArrecadacaoNossoNumeroAction">
			
			<forward name="consultaDocumentoNossoNumero"
				path="/jsp/arrecadacao/arrecadacao_documento_consultar_nosso_numero.jsp" />
		</action>
		
		<action name="ConsultarDocumentoArrecadacaoNossoNumeroActionForm"
			type="gcom.gui.arrecadacao.ConsultarDocumentoArrecadacaoNossoNumeroAction"
			validate="false" path="/consultarNossoNumeroAction" >
			
			<forward name="consultaDocumentoNossoNumero"
				path="/jsp/arrecadacao/arrecadacao_documento_consultar_nosso_numero.jsp" />
		</action>
	</action-mappings>

	<message-resources null="false"
		parameter="gcom.properties.application" />
</struts-config>