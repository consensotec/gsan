				path="/jsp/cobranca/spcserasa/negativador_movimento_exclusao_resumo.jsp" />
		
		</action>

		<action name="GerarMovimentoExclusaoNegativacaoActionForm"
		
			type="gcom.gui.cobranca.spcserasa.GerarMovimentoExclusaoNegativacaoAction"
			input="/gerarMovimentoExclusaoNegativacaoAction.do"
			path="/gerarMovimentoExclusaoNegativacaoAction" >

	<forward name="exibirGerarMovimentoExclusaoNegativacao"
				path="/jsp/cobranca/spcserasa/negativador_movimento_exclusao_gerar.jsp" />

	<forward name="gerarMovimentoExclusaoNegativacao"
				path="/jsp/cobranca/spcserasa/negativador_movimento_exclusao_resumo.jsp" />
		</action>

	</action-mappings>

</struts-config>