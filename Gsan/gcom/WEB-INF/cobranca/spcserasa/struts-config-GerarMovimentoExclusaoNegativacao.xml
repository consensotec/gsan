	</form-beans>

	<!--******************* PÁGINAS GLOBAIS *******************-->
	<global-forwards />

	<action-mappings>
		<!--******************* PÁGINA PRINCIPAL *******************-->

		<action name="GerarMovimentoExclusaoNegativacaoActionForm"
		
			type="gcom.gui.cobranca.spcserasa.ExibirGerarMovimentoExclusaoNegativacaoAction"
			input="/exibirGerarMovimentoExclusaoNegativacaoAction.do"
			path="/exibirGerarMovimentoExclusaoNegativacaoAction" >

