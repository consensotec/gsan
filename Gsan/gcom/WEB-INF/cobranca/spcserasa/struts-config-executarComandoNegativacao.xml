<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean
		    name="ExecutarComandoNegativacaoActionForm"
			type="gcom.gui.cobranca.spcserasa.ExecutarComandoNegativacaoActionForm"/> 
	</form-beans>

	<!--******************* PÁGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>
				
			
		<action name="ExecutarComandoNegativacaoActionForm"
			type="gcom.gui.cobranca.spcserasa.ExecutarComandoNegativacaoAction" 
			validate="true"
			scope="session"
			path="/executarComandoNegativacaoAction">
		</action>
 
	</action-mappings>

	<message-resources null="false" parameter="gcom.properties.application" />

</struts-config>