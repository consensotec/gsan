<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="ResultadoPesquisaPermissaoEspecialActionForm"
			type="gcom.gui.seguranca.acesso.ResultadoPesquisaPermissaoEspecialActionForm" />	
	</form-beans>
	<!--******************* PÁGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>

		<action name="ResultadoPesquisaPermissaoEspecialActionForm"
			type="gcom.gui.seguranca.acesso.ExibirResultadoPesquisaPermissaoEspecialAction"
			validate="false" scope="session"
			path="/exibirResultadoPesquisaPermissaoEspecialAction">
			<forward name="resultadoPesquisaPermissaoEspecial"
				path="/jsp/seguranca/acesso/permissao_especial_resultado_pesquisa.jsp" />
		</action>
	

		
		
	</action-mappings>
	<message-resources null="false"
		parameter="gcom.properties.application" />
</struts-config>