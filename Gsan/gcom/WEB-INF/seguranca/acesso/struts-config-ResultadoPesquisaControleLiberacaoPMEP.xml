<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="ResultadoPesquisaControleLiberacaoPMEPActionForm"
			type="gcom.gui.seguranca.acesso.ResultadoPesquisaControleLiberacaoPMEPActionForm" />	
	</form-beans>
	<!--******************* PÁGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>

		<action name="ResultadoPesquisaControleLiberacaoPMEPActionForm"
			type="gcom.gui.seguranca.acesso.ExibirResultadoPesquisaControleLiberacaoPMEPAction"
			validate="false" scope="session"
			path="/exibirResultadoPesquisaControleLiberacaoPMEPAction">
			<forward name="resultadoPesquisaControleLiberacaoPMEP"
				path="/jsp/seguranca/acesso/controle_liberacao_PMEP_resultado_pesquisa.jsp" />
		</action>		
		
	</action-mappings>
	<message-resources null="false"
		parameter="gcom.properties.application" />
</struts-config>
