<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>


	<form-beans>
		<form-bean name="GerarRelatorioLogradouroPorMunicipioActionForm"
			type="gcom.gui.relatorio.cadastro.endereco.GerarRelatorioLogradouroPorMunicipioActionForm" />
	</form-beans>

	<!--******************* PAGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>
		
		<action name="GerarRelatorioLogradouroPorMunicipioActionForm"
			type="gcom.gui.relatorio.cadastro.endereco.ExibirGerarRelatorioLogradourosPorMunicipioAction"
			validate="true" 			
			path="/exibirGerarRelatorioLogradourosPorMunicipioAction">
			
			<forward name="exibirGerarRelatorioLogradourosPorMunicipio" 
				path="/jsp/relatorio/cadastro/relatorio_logradouro_por_municipio_gerar.jsp" />
				
		</action>
		
		<action name="GerarRelatorioLogradouroPorMunicipioActionForm"
			type="gcom.gui.relatorio.cadastro.endereco.GerarRelatorioLogradouroPorMunicipioAction"
			validate="true" 
			path="/gerarRelatorioLogradouroPorMunicipioAction" />		
		
	</action-mappings>

</struts-config>