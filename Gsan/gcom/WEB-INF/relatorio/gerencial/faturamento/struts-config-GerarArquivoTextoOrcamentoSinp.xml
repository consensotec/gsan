			type="gcom.gui.relatorio.gerencial.faturamento.ExibirGerarArquivoOrcamentoSinpAction"
			validate="true" 
			path="/exibirGerarArquivoOrcamentoSinpAction">
			
			<forward name="gerarArquivoOrcamentoSinp" 
				path="/jsp/relatorio/gerencial/gerar_arquivo_orcamento_sinp.jsp" />
				
		</action>
		
		<action name="GerarArquivoOrcamentoSinpActionForm"
			type="gcom.gui.relatorio.gerencial.faturamento.GerarArquivoOrcamentoSinpAction"
			validate="true" 
			path="/gerarArquivoOrcamentoSinpAction" />
			
		
	</action-mappings>

</struts-config>