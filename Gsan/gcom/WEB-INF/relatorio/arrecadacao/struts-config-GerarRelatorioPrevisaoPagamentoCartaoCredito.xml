		<action name="GerarRelatorioPrevisaoPagamentoCartaoCreditoActionForm"
			type="gcom.gui.relatorio.arrecadacao.GerarRelatorioPrevisaoPagamentoCartaoCreditoAction"
			validate="true"						
			path="/gerarRelatorioPrevisaoPagamentoCartaoCreditoAction" />				

