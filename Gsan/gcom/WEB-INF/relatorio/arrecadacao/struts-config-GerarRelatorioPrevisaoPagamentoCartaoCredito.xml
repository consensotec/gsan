			type="gcom.gui.relatorio.arrecadacao.ExibirGerarRelatorioPrevisaoPagamentoCartaoCreditoAction"			
			validate="true"	
			path="/exibirGerarRelatorioPrevisaoPagamentoCartaoCreditoAction">			
