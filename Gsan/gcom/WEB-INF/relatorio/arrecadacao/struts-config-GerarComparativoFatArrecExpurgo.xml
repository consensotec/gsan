			type="gcom.gui.relatorio.arrecadacao.ExibirGerarComparativoFatArrecExpurgoAction"
			validate="true" 
			path="/exibirGerarComparativoFatArrecExpurgoAction">
			
			<forward name="exibirGerarComparativoFatArrecExpurgoAction" 
				path="/jsp/relatorio/arrecadacao/relatorio_comparativo_fat_arrec_expurgo_gerar.jsp" />
				
		</action>
		
		<action name="GerarComparativoFatArrecExpurgoActionForm"
			type="gcom.gui.relatorio.arrecadacao.GerarComparativoFatArrecExpurgoAction"
			validate="true" 
			path="/gerarComparativoFatArrecExpurgoAction" />		
		
	</action-mappings>

</struts-config>