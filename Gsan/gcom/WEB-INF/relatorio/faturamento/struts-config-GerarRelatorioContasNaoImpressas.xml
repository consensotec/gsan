<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>


	<form-beans>
		<form-bean name="GerarRelatorioContasNaoImpressasActionForm"
			type="gcom.gui.relatorio.faturamento.GerarRelatorioContasNaoImpressasActionForm" />
	</form-beans>

	<!--******************* PAGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>
		
		<action name="GerarRelatorioContasNaoImpressasActionForm"
			type="gcom.gui.relatorio.faturamento.ExibirGerarRelatorioContasNaoImpressasAction"
			validate="true" 
			path="/exibirGerarRelatorioContasNaoImpressasAction">
			
			<forward name="exibirGerarRelatorioContasNaoImpressas" 
				path="/jsp/relatorio/faturamento/relatorio_contas_nao_impressas_gerar.jsp" />
				
		</action>
		
		<action name="GerarRelatorioContasNaoImpressasActionForm"
			type="gcom.gui.relatorio.faturamento.GerarRelatorioContasNaoImpressasAction"
			validate="true" 
			path="/gerarRelatorioContasNaoImpressasAction" />
		
	</action-mappings>

</struts-config>