<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<form-beans>
		<form-bean
			name="ExibirLojasAtendimentoPresencialPortalCaerActionForm"
			type="gcom.gui.portal.caer.ExibirLojasAtendimentoPresencialPortalCaerActionForm" />
	</form-beans>

	<!--******************* PÃGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>

		<action
			name="ExibirLojasAtendimentoPresencialPortalCaerActionForm"
			type="gcom.gui.portal.caer.ExibirLojasAtendimentoPresencialPortalCaerAction"
			validate="false" scope="session"
			path="/exibirLojasAtendimentoPresencialPortalCaerAction">

			<forward
				name="exibirLojasAtendimentoPresencialPortalCaerAction"
				path="/jsp/portal/caer/lojas_atendimento_presencial_portal_caer.jsp" />

		</action>

	</action-mappings>

	<message-resources null="false"
		parameter="gcom.properties.application" />
</struts-config>

