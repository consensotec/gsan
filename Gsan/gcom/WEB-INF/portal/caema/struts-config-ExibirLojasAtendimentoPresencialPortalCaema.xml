<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<form-beans>
		<form-bean
			name="ExibirLojasAtendimentoPresencialPortalCaemaActionForm"
			type="gcom.gui.portal.caema.ExibirLojasAtendimentoPresencialPortalCaemaActionForm" />
	</form-beans>

	<!--******************* PÁGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>

		<action
			name="ExibirLojasAtendimentoPresencialPortalCaemaActionForm"
			type="gcom.gui.portal.caema.ExibirLojasAtendimentoPresencialPortalCaemaAction"
			validate="false" scope="session"
			path="/exibirLojasAtendimentoPresencialPortalCaemaAction">

			<forward
				name="exibirLojasAtendimentoPresencialPortalCaemaAction"
				path="/jsp/portal/caema/lojas_atendimento_presencial_portal_caema.jsp" />

		</action>

	</action-mappings>

	<message-resources null="false"
		parameter="gcom.properties.application" />
</struts-config>

