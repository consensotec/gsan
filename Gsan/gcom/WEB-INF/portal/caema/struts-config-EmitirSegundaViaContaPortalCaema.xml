			type="gcom.gui.portal.caema.EmitirSegundaViaContaPortalCaemaAction"
			validate="false" scope="session"
			path="/emitirSegundaViaContaPortalCaemaAction">
			<forward name="emitirSegundaViaContaPortalCaemaAction"
