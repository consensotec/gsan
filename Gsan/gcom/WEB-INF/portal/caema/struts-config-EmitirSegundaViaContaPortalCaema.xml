				path="/jsp/portal/caema/servicos.jsp"/>
			<forward name="erroSistemaPortal"
				path="/jsp/portal/caema/emitir_segunda_via_conta_portal_caema.jsp"/>
		</action>
	</action-mappings>

	<message-resources null="false"
		parameter="gcom.properties.application" />
</struts-config>

