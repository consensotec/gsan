<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<form-beans>
		<form-bean 
		    name="DetalharMaterialUtilizadoPopupActionForm"
			type="gcom.gui.atendimentopublico.ordemservico.DetalharMaterialUtilizadoPopupActionForm" /> 
	</form-beans>

	<!--******************* PÁGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>
		<action name="DetalharMaterialUtilizadoPopupActionForm"  
			type="gcom.gui.atendimentopublico.ordemservico.ExibirDetalharMaterialUtilizadoPopupAction" 
			validate="true"
			scope="session"
			path="/exibirDetalharMaterialUtilizadoPopupAction">

			<forward name="detalharMaterialUtilizadoPopup"
			path="/jsp/atendimentopublico/ordemservico/detalhamento_material_utilizado_popup.jsp" />
		</action>
	</action-mappings>

	<message-resources null="false"	parameter="gcom.properties.application" />
</struts-config>