-->
	<class name="gsan.faturamento.ResumoFaturamentoSimulacaoDebito" table="faturamento.res_fatu_simulacao_debi">
		<composite-id name="comp_id" class="gsan.faturamento.ResumoFaturamentoSimulacaoDebitoPK">
			<!-- bi-directional many-to-one association to Resumo Faturamento Simulacao -->
			<key-many-to-one name="resumoFaturamentoSimulacao" class="gsan.faturamento.ResumoFaturamentoSimulacao">
				<column name="rfts_id"/>
			</key-many-to-one>
			<!-- bi-directional many-to-one association to Debito Tipo -->
			<key-many-to-one name="debitoTipo" class="gsan.faturamento.debito.DebitoTipo">
				<column name="dbtp_id"/>
			</key-many-to-one>
		</composite-id>
		<property name="ultimaAlteracao" type="java.sql.Timestamp" column="rsdd_tmultimaalteracao" not-null="true" length="8"/>
		<property name="valor" type="java.math.BigDecimal" column="rsdd_vldebito" length="13"/>
	</class>
</hibernate-mapping>
