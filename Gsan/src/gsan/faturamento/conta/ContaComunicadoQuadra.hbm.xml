<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!--
    Created by the Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->
	<class name="gsan.faturamento.conta.ContaComunicadoQuadra" table="faturamento.conta_comunicado_quadra">
		<id name="id" type="java.lang.Integer" column="coqd_id">
			<generator class="sequence">
				<param name="sequence">faturamento.seq_conta_comunicado_quadra</param>
			</generator>
		</id>

		<property name="ultimaAlteracao" type="java.sql.Timestamp" column="coqd_tmultimaalteracao" length="8"/>
		
		<!-- bi-directional many-to-one association to GerenciaRegional -->
		<many-to-one name="quadra" class="gsan.cadastro.localidade.Quadra" not-null="false">
			<column name="qdra_id"/>
		</many-to-one>
		<!-- bi-directional many-to-one association to Localidade -->
		<many-to-one name="contaComunicado" class="gsan.faturamento.conta.ContaComunicado" not-null="false">
			<column name="comu_id"/>
		</many-to-one>
	</class>
</hibernate-mapping>
