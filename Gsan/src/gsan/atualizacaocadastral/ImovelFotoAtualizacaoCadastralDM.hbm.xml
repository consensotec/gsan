<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="gsan.atualizacaocadastral.ImovelFotoAtualizacaoCadastralDM" table="atualizacaocadastral.imovel_foto_atlz_cad_dm">
		<id name="id" type="java.lang.Integer" column="ifac_id">
			<generator class="sequence">
				<param name="sequence">atualizacaocadastral.seq_imovel_foto_atlz_cad_dm</param>
			</generator>
		</id>
		
		<many-to-one class="gsan.atualizacaocadastral.ImovelAtualizacaoCadastralDM" name="imovelAtualizacaoCadastralDM" not-null="true">
   			<column name="imac_id"/>
  		</many-to-one>
		
		<many-to-one name="fotoSituacao" class="gsan.atendimentopublico.ordemservico.FotoSituacaoOrdemServico">
			<column name="fsos_id"/>
		</many-to-one>
		
		<property name="fotoImovel" type="binary" column="ifac_imfoto" length="-1" not-null="true"/>
		
		<property name="ultimaAlteracao" type="java.sql.Timestamp" column="ifac_tmultimaalteracao" not-null="true" length="6"/>
		
	</class>
</hibernate-mapping>