			<column name="hili_id"/>
		</many-to-one>

		<many-to-one name="hidrometroProtecao" class="gsan.micromedicao.hidrometro.HidrometroProtecao">
			<column name="hipr_id"/>
		</many-to-one>

		<many-to-one name="imovelAtualizacaoCadastralDM" class="gsan.atualizacaocadastral.ImovelAtualizacaoCadastralDM" not-null="true">
