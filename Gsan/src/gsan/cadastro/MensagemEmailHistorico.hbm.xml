<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="gsan.cadastro.MensagemEmailHistorico" table="cadastro.mensagem_email_historico">
		<id name="id" type="java.lang.Integer" column="mehi_id">
			<generator class="sequence">
				<param name="sequence">cadastro.seq_mensagem_email_historico</param>
			</generator>
		</id>
		<property name="descricaoMensagem" type="java.lang.String" column="mehi_dsmensagem" not-null="true"/>
		<property name="emailDestino" type="java.lang.String" column="mehi_dsemaildestino" not-null="true" length="40"/>
		<property name="dataPrevisaoEnvio" type="java.sql.Timestamp" column="mehi_dtprevistaenvio" length="8"/>
		<property name="dataLimiteEnvio" type="java.sql.Timestamp" column="mehi_dtlimiteenvio" length="8"/>
		<property name="dataEnvio" type="java.sql.Timestamp" column="mehi_dtenvio" length="8"/>
		<property name="quantidadeTentativasEnvio" type="java.lang.Integer" column="mehi_qttentativasenvio" />
		<property name="ultimaAlteracao" type="java.sql.Timestamp" column="mehi_dtultimaalteracao" not-null="true" length="8"/>
		<property name="anoMesReferenciaConta" type="java.lang.Integer" column="mehi_amreferenciaconta" />
		
		<many-to-one name="cliente" class="gsan.cadastro.cliente.Cliente">
		    <column name="clie_id" />
		</many-to-one>
		<many-to-one name="parametroMensagemSMSEmail" class="gsan.cadastro.ParametrosMSGSMSEmail">
		    <column name="pmse_id" />
		</many-to-one>
		<many-to-one name="acaoCobranca" class="gsan.cobranca.CobrancaAcao">
		    <column name="cbac_id" />
		</many-to-one>
		<many-to-one name="conta" class="gsan.faturamento.conta.Conta">
		    <column name="cnta_id" />
		</many-to-one>
		<many-to-one name="grupoFaturamento" class="gsan.faturamento.FaturamentoGrupo">
		    <column name="ftgr_id" />
		</many-to-one>
		<many-to-one name="cobrancaAcaoAtividadeComando" class="gsan.cobranca.CobrancaAcaoAtividadeComando">
			<column name="cacm_id"/>
		</many-to-one>
		<many-to-one name="cobrancaDocumento" class="gsan.cobranca.CobrancaDocumento" not-null="false">
			<column name="cbdo_id"/>
		</many-to-one>
		
	</class>
</hibernate-mapping>
