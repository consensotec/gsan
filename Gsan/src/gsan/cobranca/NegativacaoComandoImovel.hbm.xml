<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!-- 

    Created by the Middlegen Hibernate plugin 2.1



    http://boss.bekk.no/boss/middlegen/

    http://www.hibernate.org/

-->
	<class name="gsan.cobranca.NegativacaoComandoImovel" table="cobranca.negativacao_comando_imov">
		<id name="id" type="java.lang.Integer" column="ncim_id">
			<generator class="sequence">
				<param name="sequence">cobranca.seq_negativacao_comando_imov</param>
			</generator>
		</id>
		<property name="ultimaAlteracao" type="java.sql.Timestamp" column="ncim_tmultimaalteracao" length="8"/>
		<!-- Associations -->
		<!-- bi-directional many-to-one association to Usuario -->
		<many-to-one name="usuario" class="gsan.seguranca.acesso.usuario.Usuario" not-null="false">
			<column name="usur_id"/>
		</many-to-one>
		
		<many-to-one name="imovel" class="gsan.cadastro.imovel.Imovel" not-null="true">
			<column name="imov_id"/>
		</many-to-one>
		
		<many-to-one name="cliente" class="gsan.cadastro.cliente.Cliente" not-null="false">
			<column name="clie_id"/>
		</many-to-one>
		
		<many-to-one name="negativacaoComando" class="gsan.cobranca.NegativacaoComando" not-null="true">
			<column name="ngcm_id"/>
		</many-to-one>
		
		
	</class>
</hibernate-mapping>