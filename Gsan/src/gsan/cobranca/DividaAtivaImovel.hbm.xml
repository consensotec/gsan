			<column name="dacr_id"/>
		</many-to-one>
		<!-- bi-directional one-to-many association to DividaAtivaCriterio -->
		<!-- bi-directional many-to-one association to Imovel -->
		<many-to-one name="imovel" class="gsan.cadastro.imovel.Imovel" not-null="true">
			<column name="imov_id"/>
		</many-to-one>
		<!-- bi-directional one-to-many association to Imovel -->
		<!-- bi-directional many-to-one association to Cliente -->
		<many-to-one name="cliente" class="gsan.cadastro.cliente.Cliente">
			<column name="clie_id"/>
		</many-to-one>
		<!-- bi-directional one-to-many association to Cliente -->
	</class>
