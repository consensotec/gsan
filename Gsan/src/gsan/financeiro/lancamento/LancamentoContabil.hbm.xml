<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!--
    Created by the Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->
	<class name="gsan.financeiro.lancamento.LancamentoContabil" table="financeiro.lancamento_contabil">
		<id name="id" type="java.lang.Integer" column="lcnb_id">
			<generator class="sequence">
				<param name="sequence">financeiro.seq_lancamento_contabil</param>
			</generator>
		</id>
		<property name="anoMes" type="int" column="lcnb_amlancamento" not-null="true" length="4"/>
		<property name="ultimaAlteracao" type="java.sql.Timestamp" column="lcnb_tmultimaalteracao" length="8"/>
		<!-- associations -->
		<!-- bi-directional many-to-one association to LancamentoResumo -->
		<many-to-one name="lancamentoOrigem" class="gsan.financeiro.lancamento.LancamentoOrigem" not-null="true">
			<column name="lcor_id"/>
		</many-to-one>
		<!-- bi-directional many-to-one association to LancamentoResumo -->
		<many-to-one name="lancamentoTipo" class="gsan.financeiro.lancamento.LancamentoTipo" not-null="true">
			<column name="lctp_id"/>
		</many-to-one>
		<!-- bi-directional many-to-one association to Localidade -->
		<many-to-one name="localidade" class="gsan.cadastro.localidade.Localidade" not-null="true">
			<column name="loca_id"/>
		</many-to-one>
		<!-- bi-directional many-to-one association to RecebimentoTipo -->
		<many-to-one name="recebimentoTipo" class="gsan.arrecadacao.RecebimentoTipo" not-null="true">
			<column name="rctp_id"/>
		</many-to-one>
	</class>
</hibernate-mapping>
