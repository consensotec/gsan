			<column name="fncd_id"/>
		</many-to-one>
		<!-- bi-directional many-to-one association to Funcionalidade -->
		<many-to-one name="operacaoTipo" class="gcom.seguranca.acesso.OperacaoTipo" not-null="true">
			<column name="optp_id"/>
		</many-to-one>
		<!-- bi-directional many-to-one association to Funcionalidade -->
		<many-to-one name="idOperacaoPesquisa" class="gcom.seguranca.acesso.Operacao" not-null="true">
			<column name="oper_idoperacaopesquisa"/>
		</many-to-one>
		<!-- bi-directional many-to-one association to Funcionalidade -->
		<many-to-one name="tabelaColuna" class="gcom.seguranca.transacao.TabelaColuna" not-null="true">
			<column name="tbco_id"/>
		</many-to-one>
		<many-to-one name="argumentoPesquisa" class="gcom.seguranca.transacao.TabelaColuna" not-null="true">
			<column name="tbco_idargumento"/>
		</many-to-one>
