		<set name="gerQuadra" lazy="true" inverse="true" cascade="all">
			<key>
				<column name="rota_id"/>
			</key>
			<one-to-many class="gcom.gerencial.cadastro.localidade.GQuadra"/>
		</set>
		<!-- bi-directional one-to-many association to UnResumoIndicadorDesempenhoMicromedicao -->
