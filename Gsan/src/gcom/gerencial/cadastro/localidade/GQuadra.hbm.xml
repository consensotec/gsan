			<one-to-many class="gcom.gerencial.micromedicao.UnResumoIndicadorDesempenhoMicromedicao"/>
		</set>
		<many-to-one name="gerDistritoOperacional" class="gcom.gerencial.operacional.GDistritoOperacional" not-null="false">
