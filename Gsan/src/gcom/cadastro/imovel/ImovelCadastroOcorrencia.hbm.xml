<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->
	<class name="gcom.cadastro.imovel.ImovelCadastroOcorrencia" table="cadastro.imovel_cad_ocorrencia">
		<id name="id" type="java.lang.Integer" column="icor_id">
			<generator class="sequence">
				<param name="sequence">cadastro.seq_imovel_cad_ocorrencia</param>
			</generator>
		</id>
		<property name="dataOcorrencia" type="java.sql.Date" column="icor_dtocorrenciacadastro" not-null="true" length="4"/>
		<property name="fotoOcorrencia" type="binary" column="icor_fotoocorrenciacadastro" length="-1"/>
		<property name="ultimaAlteracao" type="java.sql.Timestamp" column="icor_tmultimaalteracao" not-null="true" length="8"/>
		<!-- Associations -->
		<!-- bi-directional many-to-one association to Imovel -->
		<many-to-one name="imovel" class="gcom.cadastro.imovel.Imovel" not-null="true">
			<column name="imov_id"/>
		</many-to-one>
		<!-- bi-directional many-to-one association to CadastroOcorrencia -->
		<many-to-one name="cadastroOcorrencia" class="gcom.cadastro.imovel.CadastroOcorrencia" not-null="true">
			<column name="cocr_id"/>
		</many-to-one>
	</class>
</hibernate-mapping>
