<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">    
	<hibernate-mapping>
		<class name="gcom.cadastro.MotivoRetiradaCobranca" table="cadastro.motivo_retirada_cobranca">
			<id name="id" type="java.lang.Integer" column="mrcb_id" />
			<property name="descricao" type="java.lang.String" column="mrcb_dsmotivoretirada" length="100"/>
			<property name="ultimaAlteracao" type="java.sql.Timestamp" column="mrcb_tmultimaalteracao" length="8"/>
		</class>
	</hibernate-mapping>
