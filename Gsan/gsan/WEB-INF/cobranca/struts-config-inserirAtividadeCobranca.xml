<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="InserirAtividadeCobrancaActionForm"
			type="gsan.gui.cobranca.InserirAtividadeCobrancaActionForm" />					
	</form-beans>

	<!--******************* PÁGINAS GLOBAIS *******************-->
	<global-forwards />

	<action-mappings>
		<!--******************* PÁGINA PRINCIPAL *******************-->
		
		<action name="InserirAtividadeCobrancaActionForm"
			type="gsan.gui.cobranca.ExibirInserirAtividadeCobrancaAction"
			validate="true" scope="session"
			path="/exibirInserirAtividadeCobrancaAction">
			<forward name="exibirInserirAtividadeCobrancaAction"
				path="/jsp/cobranca/atividade_cobranca_inserir.jsp" />
		</action>
		<action name="InserirAtividadeCobrancaActionForm"
			type="gsan.gui.cobranca.InserirAtividadeCobrancaAction"
			validate="true" scope="session"
			path="/inserirAtividadeCobrancaAction">
		</action>

	</action-mappings>

</struts-config>