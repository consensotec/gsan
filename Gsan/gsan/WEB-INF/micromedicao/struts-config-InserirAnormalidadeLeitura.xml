<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="InserirAnormalidadeLeituraActionForm"
			type="gsan.gui.micromedicao.leitura.InserirAnormalidadeLeituraActionForm" />
	</form-beans>
	<global-forwards />
	<action-mappings>

		<action name="InserirAnormalidadeLeituraActionForm"
				type="gsan.gui.micromedicao.leitura.ExibirInserirAnormalidadeLeituraAction"
				validate="true" 
				scope="session"
				path="/exibirInserirAnormalidadeLeituraAction">
			<forward name="inserirAnormalidadeLeitura"
					 path="/jsp/micromedicao/leitura/anormalidade_leitura_inserir.jsp" />
		</action>
		<action name="InserirAnormalidadeLeituraActionForm"
				type="gsan.gui.micromedicao.leitura.InserirAnormalidadeLeituraAction"
				validate="true" 
				scope="session"
				path="/inserirAnormalidadeLeituraAction">
		</action>

	</action-mappings>
	<message-resources null="false" parameter="gsan.properties.application" />
</struts-config>