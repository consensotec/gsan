<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="AtualizarContaNovoClienteActionForm"
			type="gsan.gui.faturamento.conta.AtualizarContaNovoClienteActionForm" />
	</form-beans>

	<global-forwards />
	<action-mappings>

		<action name="AtualizarContaNovoClienteActionForm"  
			type="gsan.gui.faturamento.conta.ExibirAtualizarContaNovoClienteAction" 
			validate="false"
			scope="session"
			path="/exibirAtualizarContaNovoClienteAction">

			<forward name="exibirAtualizarContaNovoCliente"
			path="/jsp/faturamento/conta/conta_novo_cliente_atualizar.jsp" />

		</action>

		<action name="AtualizarContaNovoClienteActionForm"
			type="gsan.gui.faturamento.conta.AtualizarContaNovoClienteAction"
			validate="true" 
			scope="session"
			path="/atualizarContaNovoClienteAction">
			
			
			<forward name="atualizarContaNovoCliente"
				path="/atualizarContaNovoClienteAction.do" />
			
		</action>

	</action-mappings>

	<message-resources null="false"
		parameter="gsan.properties.application" />
</struts-config>

