<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="InserirVigenciaSimulacaoTarifaConsumoActionForm"
			type="gsan.gui.faturamento.consumotarifa.InserirVigenciaSimulacaoTarifaConsumoActionForm" />
	</form-beans>

	<!--******************* PÁGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>

		<action name="InserirVigenciaSimulacaoTarifaConsumoActionForm"
			type="gsan.gui.faturamento.consumotarifa.ExibirInserirVigenciaSimulacaoTarifaConsumoAction"
			validate="true" scope="session"
			path="/exibirInserirVigenciaSimulacaoTarifaConsumoAction">
			<forward name="vigenciaSimulacaoTarifaConsumoInserir"
				path="/jsp/faturamento/vigencia_simulacao_tarifa_consumo_inserir.jsp" />
		</action>

		<action name="InserirVigenciaSimulacaoTarifaConsumoActionForm"
			type="gsan.gui.faturamento.consumotarifa.InserirVigenciaSimulacaoTarifaConsumoAction"
			validate="true" scope="session" 
			path="/inserirVigenciaSimulacaoTarifaConsumoAction" >
		
		</action>

	</action-mappings>
	<message-resources null="false"
		parameter="gsan.properties.application" />
</struts-config>