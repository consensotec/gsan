<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<form-beans>
		<form-bean name="LiberarLocalidadeAtualizacaoCadastralActionForm"
			type="gsan.gui.atualizacaocadastral.LiberarLocalidadeAtualizacaoCadastralActionForm" />
	</form-beans>

	<!--******************* PÁGINAS GLOBAIS *******************-->
	<global-forwards />
	
	<action-mappings>
		<action name="LiberarLocalidadeAtualizacaoCadastralActionForm"
			type="gsan.gui.atualizacaocadastral.ExibirLiberarLocalidadeAtualizacaoCadastralAction"
			validate="false" scope="session" path="/exibirLiberarLocalidadeAtualizacaoCadastralAction">

			<forward name="liberarLocalidadeAtualizacaoCadastral"
				path="/jsp/atualizacaocadastral/atualizacao_cadastral_localidade_liberar.jsp" />
		</action>
		
		<action name="LiberarLocalidadeAtualizacaoCadastralActionForm"
			type="gsan.gui.atualizacaocadastral.LiberarLocalidadeAtualizacaoCadastralAction"
			validate="false" scope="session"
			path="/liberarLocalidadeAtualizacaoCadastralAction">
			
		</action>
	</action-mappings>
	
	<message-resources null="false"
		parameter="gsan.properties.application" />
</struts-config>