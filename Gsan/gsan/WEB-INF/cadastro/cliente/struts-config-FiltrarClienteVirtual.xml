<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="FiltrarClienteVirtualActionForm"
			type="gsan.gui.cadastro.cliente.FiltrarClienteVirtualActionForm" />


	</form-beans>

	<!--******************* Pï¿½GINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>

		<action name="FiltrarClienteVirtualActionForm"  
			type="gsan.gui.cadastro.cliente.ExibirFiltrarClienteVirtualAction" 
			validate="false"
			scope="session"
			path="/exibirFiltrarClienteVirtualAction">

			<forward name="exibirFiltrarClienteVirtualAction"
			path="/jsp/cadastro/cliente/cliente_virtual_filtrar.jsp" />

		</action>

	</action-mappings>

	<message-resources null="false"
		parameter="gsan.properties.application" />
</struts-config>

