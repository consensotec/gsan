		</action>		
		<action name="GerarLancamentosContabeisArrecadacaoActionForm"
				type="gsan.gui.financeiro.GerarLancamentoContabeisArrecadacaoAction"
				validate="false" 
				scope="session"
				path="/gerarLancamentosContabeisArrecadacaoAction">
