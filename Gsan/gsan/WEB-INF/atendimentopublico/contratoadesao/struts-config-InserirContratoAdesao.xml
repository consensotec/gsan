<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="InserirContratoAdesaoActionForm"
			type="gsan.gui.atendimentopublico.contratoadesao.InserirContratoAdesaoActionForm" />
	</form-beans>

	<!--******************* PÁGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>

		<action name="InserirContratoAdesaoActionForm"
			type="gsan.gui.atendimentopublico.contratoadesao.ExibirInserirContratoAdesaoAction"
			validate="false" scope="session"
			path="/exibirInserirContratoAdesaoAction">
			<forward name="inserirContratoAdesao"
				path="/jsp/atendimentopublico/contratoadesao/contrato_adesao_inserir.jsp" />
		</action>

		<action name="InserirContratoAdesaoActionForm"
			type="gsan.gui.atendimentopublico.contratoadesao.InserirContratoAdesaoAction"
			validate="false" scope="session"
			path="/inserirContratoAdesaoAction">			       
		</action>

	</action-mappings>
	<message-resources null="false"
		parameter="gsan.properties.application" />
</struts-config>