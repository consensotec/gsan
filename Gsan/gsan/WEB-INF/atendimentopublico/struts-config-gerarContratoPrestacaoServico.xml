<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="GerarContratoPrestacaoServicoActionForm"
			type="gsan.gui.atendimentopublico.GerarContratoPrestacaoServicoActionForm" />					
	</form-beans>

	<!--******************* PÁGINAS GLOBAIS *******************-->
	<global-forwards />

	<action-mappings>
	
		<!--******************* PÁGINA PRINCIPAL *******************-->
		<!-- [UC0XXX] GERAR CONTRATO PRESTAÇÃO SERVIÇO Auhtor: Rafael Corrêa Data:31/03/2006-->
		<action name="GerarContratoPrestacaoServicoActionForm"
			type="gsan.gui.atendimentopublico.ExibirGerarContratoPrestacaoServicoAction"
			validate="false" scope="session"
			path="/exibirGerarContratoPrestacaoServicoAction">
			<forward name="exibirGerarContratoPrestacaoServico"
				path="/jsp/atendimentopublico/contrato_prestacao_servico_gerar.jsp" />
		</action>
		<action name="GerarContratoPrestacaoServicoActionForm"
			type="gsan.gui.relatorio.atendimentopublico.GerarRelatorioContratoPrestacaoServicoAction"
			validate="false" scope="session"
			path="/gerarRelatorioContratoPrestacaoServicoAction" />
			
		<action name="GerarContratoPrestacaoServicoActionForm"
			type="gsan.gui.relatorio.atendimentopublico.EmitirContratoAdesaoAction"
			validate="false" scope="session"
			path="/emitirContratoAdesaoAction" />

	</action-mappings>

</struts-config>