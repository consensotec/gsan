<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<form-beans>
		<form-bean 
		    name="FiltrarOcorrenciaOperacionalActionForm"
			type="gsan.gui.atendimentopublico.registroatendimento.FiltrarOcorrenciaOperacionalActionForm" /> 
	</form-beans>


	<!--******************* P?GINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>
				
			
		<action name="FiltrarOcorrenciaOperacionalActionForm"  
			type="gsan.gui.atendimentopublico.registroatendimento.ExibirFiltrarOcorrenciaOperacionalAction" 
			validate="true"
			scope="session"
			path="/exibirFiltrarOcorrenciaOperacionalAction">
			<forward name="filtrarOcorrenciaOperacional"
			path="/jsp/atendimentopublico/registroatendimento/ocorrencia_operacional_filtrar.jsp" />
		</action>

		<action name="FiltrarOcorrenciaOperacionalActionForm"
			type="gsan.gui.atendimentopublico.registroatendimento.FiltrarOcorrenciaOperacionalAction"
			validate="true" scope="session"
			path="/filtrarOcorrenciaOperacionalAction">
			<forward name="exibirManterOcorrenciaOperacionalAction"
			path="/exibirManterOcorrenciaOperacionalAction.do" />
		</action>
 
	</action-mappings>

	<message-resources null="false"
		parameter="gsan.properties.application" />
		
</struts-config>