<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<form-beans>
		<form-bean name="ConsultarImovelActionForm"
			type="gsan.gui.cadastro.imovel.ConsultarImovelActionForm" />
	</form-beans>

	<!--****************** P?GINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>
		
		<action name="ConsultarImovelActionForm"
			type="gsan.gui.atendimentopublico.ExibirVisualizarCoordenadasImpressaoSimultaneaPopupAction"
			validate="false" scope="session"
			path="/exibirVisualizarCoordenadasImpressaoSimultaneaPopupAction">
			<forward name="visualizarCoordenadas"
				path="/jsp/atendimentopublico/impressao_simultanea_coordenadas_visualizar_popup.jsp" />
			
		</action>
		
	</action-mappings>

	<message-resources null="false"
		parameter="gsan.properties.application" />
</struts-config>

