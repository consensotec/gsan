<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<form-beans>

		<form-bean name="ManterLigacaoAguaSituacaoActionForm"
			type="gsan.gui.atendimentopublico.ligacaoagua.ManterLigacaoAguaSituacaoActionForm" />
		<form-bean name="AtualizarLigacaoAguaSituacaoActionForm"
			type="gsan.gui.atendimentopublico.ligacaoagua.AtualizarLigacaoAguaSituacaoActionForm" />
		<form-bean name="FiltrarLigacaoAguaSituacaoActionForm"
			type="gsan.gui.atendimentopublico.ligacaoagua.FiltrarLigacaoAguaSituacaoActionForm" />

	</form-beans>

	<global-forwards />

	<action-mappings>
		<action name="ManutencaoRegistroActionForm"
			type="gsan.gui.atendimentopublico.ligacaoagua.ExibirManterLigacaoAguaSituacaoAction"
			validate="false" scope="session"
			path="/exibirManterLigacaoAguaSituacaoAction">

			<forward name="exibirManterLigacaoAguaSituacao"
				path="/jsp/atendimentopublico/ligacaoagua/ligacao_agua_situacao_manter.jsp" />

			<forward name="exibirAtualizarLigacaoAguaSituacao"
				path="/exibirAtualizarLigacaoAguaSituacaoAction.do" />
		</action>
		
		<action name="FiltrarLigacaoAguaSituacaoActionForm"
			type="gsan.gui.relatorio.atendimentopublico.GerarRelatorioLigacaoAguaSituacaoManterAction"
			validate="true" scope="session"
			path="/gerarRelatorioLigacaoAguaSituacaoManterAction" />

		<action name="AtualizarLigacaoAguaSituacaoActionForm"
			type="gsan.gui.atendimentopublico.ligacaoagua.ExibirAtualizarLigacaoAguaSituacaoAction"
			validate="false" scope="session"
			path="/exibirAtualizarLigacaoAguaSituacaoAction">

			<forward name="ligacaoAguaSituacaoAtualizar"
				path="/jsp/atendimentopublico/ligacaoagua/ligacao_agua_situacao_atualizar.jsp" />
		</action>

		<action name="AtualizarLigacaoAguaSituacaoActionForm"
			type="gsan.gui.atendimentopublico.ligacaoagua.AtualizarLigacaoAguaSituacaoAction"
			validate="true" scope="session"
			path="/atualizarLigacaoAguaSituacaoAction">

		</action>

		<action name="ManutencaoRegistroActionForm"
			type="gsan.gui.atendimentopublico.ligacaoagua.RemoverLigacaoAguaSituacaoAction"
			 scope="request" path="/removerLigacaoAguaSituacaoAction">

		</action>
	</action-mappings>

	<message-resources null="false"
		parameter="gsan.properties.application" />

</struts-config>