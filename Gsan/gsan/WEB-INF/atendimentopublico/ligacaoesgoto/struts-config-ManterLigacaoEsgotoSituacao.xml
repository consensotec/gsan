<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<form-beans>

		<form-bean name="ManterLigacaoEsgotoSituacaoActionForm"
			type="gsan.gui.atendimentopublico.ligacaoesgoto.ManterLigacaoEsgotoSituacaoActionForm" />
		<form-bean name="AtualizarLigacaoEsgotoSituacaoActionForm"
			type="gsan.gui.atendimentopublico.ligacaoesgoto.AtualizarLigacaoEsgotoSituacaoActionForm" />
		<form-bean name="FiltrarLigacaoEsgotoSituacaoActionForm"
			type="gsan.gui.atendimentopublico.ligacaoesgoto.FiltrarLigacaoEsgotoSituacaoActionForm" />

	</form-beans>

	<global-forwards />

	<action-mappings>
		<action name="ManutencaoRegistroActionForm"
			type="gsan.gui.atendimentopublico.ligacaoesgoto.ExibirManterLigacaoEsgotoSituacaoAction"
			validate="false" scope="session"
			path="/exibirManterLigacaoEsgotoSituacaoAction">

			<forward name="exibirManterLigacaoEsgotoSituacao"
				path="/jsp/atendimentopublico/ligacaoesgoto/ligacao_esgoto_situacao_manter.jsp" />

			<forward name="exibirAtualizarLigacaoEsgotoSituacao"
				path="/exibirAtualizarLigacaoEsgotoSituacaoAction.do" />
		</action>

		<action name="AtualizarLigacaoEsgotoSituacaoActionForm"
			type="gsan.gui.atendimentopublico.ligacaoesgoto.ExibirAtualizarLigacaoEsgotoSituacaoAction"
			validate="false" scope="session"
			path="/exibirAtualizarLigacaoEsgotoSituacaoAction">

			<forward name="ligacaoEsgotoSituacaoAtualizar"
				path="/jsp/atendimentopublico/ligacaoesgoto/ligacao_esgoto_situacao_atualizar.jsp" />
		</action>

		<action name="AtualizarLigacaoEsgotoSituacaoActionForm"
			type="gsan.gui.atendimentopublico.ligacaoesgoto.AtualizarLigacaoEsgotoSituacaoAction"
			validate="true" scope="session"
			path="/atualizarLigacaoEsgotoSituacaoAction">

		</action>

		<action name="ManutencaoRegistroActionForm"
			type="gsan.gui.atendimentopublico.ligacaoesgoto.RemoverLigacaoEsgotoSituacaoAction"
			 scope="request" path="/removerLigacaoEsgotoSituacaoAction">

		</action>
	</action-mappings>

	<message-resources null="false"
		parameter="gsan.properties.application" />

</struts-config>