<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="InserirServicoTipoActionForm"
			type="gsan.gui.atendimentopublico.ordemservico.InserirServicoTipoActionForm" />
	</form-beans>
	<global-forwards />
	<action-mappings>

		<action name="InserirServicoTipoActionForm"
				type="gsan.gui.atendimentopublico.ordemservico.ExibirInserirServicoTipoAction"
				validate="true" 
				scope="session"
				path="/exibirInserirServicoTipoAction">
			<forward name="inserirServicoTipo"
					 path="/jsp/atendimentopublico/ordemservico/servico_tipo_inserir.jsp" />
		</action>
		<action name="InserirServicoTipoActionForm"
				type="gsan.gui.atendimentopublico.ordemservico.InserirServicoTipoAction"
				validate="true" 
				scope="session"
				path="/inserirServicoTipoAction">
		</action>

	</action-mappings>
	<message-resources null="false" parameter="gsan.properties.application" />
</struts-config>