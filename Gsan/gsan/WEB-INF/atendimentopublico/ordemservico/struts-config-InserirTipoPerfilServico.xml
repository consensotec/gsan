<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="InserirTipoPerfilServicoActionForm"
			type="gsan.gui.atendimentopublico.ordemservico.InserirTipoPerfilServicoActionForm" />
	</form-beans>
	<!--******************* PÁGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>

		<action name="InserirTipoPerfilServicoActionForm"
			type="gsan.gui.atendimentopublico.ordemservico.ExibirInserirTipoPerfilServicoAction"
			validate="true" scope="session"
			path="/exibirInserirTipoPerfilServicoAction">
			<forward name="inserirTipoPerfilServico"
				path="/jsp/atendimentopublico/ordemservico/servico_perfil_tipo_inserir.jsp" />
		</action>

		<action name="InserirTipoPerfilServicoActionForm"
			type="gsan.gui.atendimentopublico.ordemservico.InserirTipoPerfilServicoAction"
			validate="true" scope="session"
			path="/inserirTipoPerfilServicoAction">
		</action>

	</action-mappings>
	<message-resources null="false"
		parameter="gsan.properties.application" />
</struts-config>