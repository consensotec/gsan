<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>  

	<form-beans>
	
		<form-bean name="AtualizarMaterialActionForm"
			type="gsan.gui.atendimentopublico.ordemservico.AtualizarMaterialActionForm" />
	
		<form-bean name="ManutencaoRegistroActionForm" 
			type="gsan.gui.ManutencaoRegistroActionForm" />
			
	</form-beans>

	<global-forwards />
	
	
	<action-mappings> 	
		<action  name="ManutencaoRegistroActionForm"  
			type="gsan.gui.atendimentopublico.ordemservico.ExibirManterMaterialAction"
			validate="false" 
			scope="session" 
			path="/exibirManterMaterialAction">
			<forward name="manterMaterial"
				path="/jsp/atendimentopublico/ordemservico/material_manter.jsp" />
			
			<forward name="exibirAtualizarMaterial"
				path="/exibirAtualizarMaterialAction.do" /> 
		</action>
		
		<action name="AtualizarMaterialActionForm"  
			type="gsan.gui.atendimentopublico.ordemservico.ExibirAtualizarMaterialAction"
			validate="false" 
			scope="session" 
			path="/exibirAtualizarMaterialAction">
			<forward name="atualizarMaterial"
				path="/jsp/atendimentopublico/ordemservico/material_atualizar.jsp" />
		</action>
		
		<action name="AtualizarMaterialActionForm"  
			type="gsan.gui.atendimentopublico.ordemservico.AtualizarMaterialAction"
			validate="false" 
			scope="session" 
			path="/atualizarMaterialAction">
		</action>		
		
		<action  name="ManutencaoRegistroActionForm"  
			type="gsan.gui.atendimentopublico.ordemservico.RemoverMaterialAction"
			validate="false" 
			scope="session" 
			path="/removerMaterialAction">
		</action>	
			
	</action-mappings>

	<message-resources null="false"
		parameter="gsan.properties.application" />
		
</struts-config>