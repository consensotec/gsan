<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<form-beans>
		<form-bean name="FiltrarConsultaCadastroCdlActionForm"
			type="gsan.seguranca.FiltrarConsultaCadastroCdlActionForm" />
	</form-beans>

	<!--******************* PÃGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>
		<action name="FiltrarConsultaCadastroCdlActionForm" 
			type="gsan.gui.seguranca.ExibirFiltrarConsultaCadastroCdlAction" 
			validate="false" 
			scope="session" 
			path="/exibirFiltrarConsultaCadastroCdlAction">
			
			<forward name="filtrarConsultaCadastroCdl"
				path="/jsp/seguranca/consulta_cadastro_cdl_filtrar.jsp" />
		</action>
		
		<action name="FiltrarConsultaCadastroCdlActionForm" 
			type="gsan.gui.seguranca.FiltrarConsultaCadastroCdlAction" 
			validate="true" 
			scope="session" 
			path="/filtrarConsultaCadastroCdlAction">
			<forward name="exibirResultadoPesquisaConsultaCadastroCdl" 
			path="/exibirResultadoPesquisaConsultaCadastroCdlAction.do" />
		</action>
		
	</action-mappings>

	<message-resources null="false"
		parameter="gsan.properties.application" />
		
</struts-config>