<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<form-beans>
		<form-bean name="ParametroSistemaActionForm"
			type="gsan.gui.seguranca.parametrosistema.ParametroSistemaActionForm" />
	
	</form-beans>

	<!--******************* PÃGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>
		<action name="ParametroSistemaActionForm" 
			type="gsan.gui.seguranca.parametrosistema.ExibirFiltrarParametroSistemaAction" 
			validate="false" 
			scope="session" 
			path="/exibirFiltrarParametroSistemaAction">
			
			<forward name="filtrarParametroSistema"
				path="/jsp/seguranca/sistemaparametro/parametro_sistema_filtrar.jsp" />
		</action>
		
		
		<action name="ParametroSistemaActionForm" 
			type="gsan.gui.seguranca.parametrosistema.FiltrarParametroSistemaAction" 
			validate="true" 
			scope="session" 
			path="/filtrarParametroSistemaAction">
			
		<forward name="exibirManterParametroSistema"
				path="/exibirManterParametroSistemaAction.do" />
			
		</action>
		
	</action-mappings>

	<message-resources null="false"
		parameter="gsan.properties.application" />
		
</struts-config>