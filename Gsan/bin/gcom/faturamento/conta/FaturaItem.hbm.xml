<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!-- 
    Created by the Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->
	<class name="gcom.faturamento.conta.FaturaItem" table="faturamento.fatura_item">
		<id name="id" type="java.lang.Integer" column="fitm_id">
			<generator class="sequence">
				<param name="sequence">faturamento.seq_fatura_item</param>
			</generator>
		</id>
		<property name="itemSequencia" type="java.lang.Integer" column="fitm_nnitemsequencia" not-null="true" length="2"/>
		<!-- 
    <property
        name="indicadorUso"
        type="java.lang.Integer"
        column="fitm_icuso"
        length="4"
    />
    -->
    <property
        name="indicadorUsoAtivo"
        type="java.lang.Integer"
        column="fitm_icativo"
        length="4"
    />
    
     
		<property name="valorConta" type="java.math.BigDecimal" column="fitm_vlconta" length="13"/>
		<property name="ultimaAlteracao" type="java.sql.Timestamp" column="fitm_tmultimaalteracao" length="8"/>
		<!-- 
    <property
        name="flag"
        type="java.lang.Short"
        column="fitm_icflag"
        length="2"
    />
     -->
		<property name="numeroConsumo" type="java.lang.Integer" column="fitm_nnconsumo" length="4" not-null="false"/>
		<property name="valorImposto" type="java.math.BigDecimal" column="fitm_vlimposto" length="13"/>
		<!-- associations -->
		<!-- bi-directional many-to-one association to Fatura -->
		<many-to-one name="fatura" class="gcom.faturamento.conta.Fatura" not-null="true">
			<column name="fatu_id"/>
		</many-to-one>
		<!-- bi-directional many-to-one association to Conta -->
		<many-to-one name="contaGeral" class="gcom.faturamento.conta.ContaGeral" not-null="true">
			<column name="cnta_id"/>
		</many-to-one>
		<!-- bi-directional many-to-one association to Imovel -->
		<many-to-one name="imovel" class="gcom.cadastro.imovel.Imovel" not-null="true">
			<column name="imov_id"/>
		</many-to-one>
	</class>
</hibernate-mapping>
