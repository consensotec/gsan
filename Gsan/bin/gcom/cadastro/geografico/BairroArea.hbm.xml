		<property name="codigoMunicipio" type="java.lang.Short" column="brar_cdmunicipio" not-null="false" length="2"/>
		<!-- Associations -->
		<!-- bi-directional many-to-one association to Bairro -->
		<many-to-one name="bairro" class="gcom.cadastro.geografico.Bairro" not-null="true">
			<column name="bair_id"/>
		</many-to-one>
		<!-- bi-directional many-to-one association to DistritoOperacional -->
