    <property
        name="ultimaAlteracao"
        type="java.sql.Timestamp"
        column="soac_tmultimaalteracao"
        not-null="true"
        length="8"
    />
    
    <property
        name="codigoSituacao"
        type="java.lang.Short"
        column="sast_cdsituacao"
        length="2"
    />
	
