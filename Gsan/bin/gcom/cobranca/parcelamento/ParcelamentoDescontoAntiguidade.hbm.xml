			<column name="pcpf_id"/>
		</many-to-one>
		<!-- bi-directional many-to-one association to ContaMotivoRevisao -->
		<many-to-one name="contaMotivoRevisao" class="gcom.faturamento.conta.ContaMotivoRevisao" not-null="false">
