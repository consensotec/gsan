			<key-many-to-one name="cobrancaSituacao" class="gcom.cobranca.CobrancaSituacao">
				<column name="cbst_id"/>
			</key-many-to-one>
		</composite-id>
		<property name="ultimaAlteracao" type="java.sql.Timestamp" column="ctsc_tmultimaalteracao" length="8"/>
	</class>
</hibernate-mapping>
